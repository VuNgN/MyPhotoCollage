<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout android:layout_gravity="center"
    android:id="@+id/wrapper_font_item"
    android:background="@android:color/transparent"
    android:layout_width="35.0dip"
    android:layout_height="35.0dip"
    android:layout_margin="5.0dip"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <com.github.pavlospt.CircleView android:id="@+id/color"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:cv_backgroundColorValue="@android:color/transparent"
        app:cv_fillColor="@android:color/transparent"
        app:cv_fillRadius="0.9"
        app:cv_strokeColorValue="@android:color/transparent"
        app:cv_strokeWidthSize="5.0"
        app:cv_subtitleText=""
        app:cv_titleText=""
        xmlns:app="http://schemas.android.com/apk/res-auto" />
</androidx.constraintlayout.widget.ConstraintLayout>